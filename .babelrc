{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "node": "current"
      }
    }],
    ["@babel/preset-react", {
      "pragma": "h",
      "pragmaFrag": "Fragment"
    }]
  ],
  "plugins": [
    ["@babel/plugin-transform-react-jsx", {
      "pragma": "h",
      "pragmaFrag": "Fragment"
    }],
    "@babel/plugin-transform-class-properties",
    "@babel/plugin-proposal-object-rest-spread"
  ]
}
