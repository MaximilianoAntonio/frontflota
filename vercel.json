{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "npm run vercel-build",
  "env": {
    "NODE_OPTIONS": "--openssl-legacy-provider --max-old-space-size=4096"
  },
  "functions": {
    "build": {
      "memory": 3008,
      "maxDuration": 30
    }
  },
  "build": {
    "env": {
      "NODE_VERSION": "18.18.0"
    }
  }
}
